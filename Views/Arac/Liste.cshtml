@{
}
@model IEnumerable<AspProject1.Models.Araclar>

@if (Model != null && Model.Any())
{
    <table>
        <tr>
            <th>Plaka</th>
            <th>Marka</th>
            <th>Model</th>
        </tr>
        @foreach (var arac in Model)
        {
            <tr>
                <td>@arac.Plaka</td>
                <td>@arac.Marka</td>
                <td>@arac.Model</td>
            </tr>
        }
    </table>
}
else
{
    <p>Henüz eklenmiş araç bulunmamaktadır.</p>
}
